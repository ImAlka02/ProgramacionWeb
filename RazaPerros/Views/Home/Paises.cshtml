@model IEnumerable<RazaPerros.Models.ViewModels.PaisesViewModel>
@{
    Layout = "Layout";
}
<section class="nav">
            <h3>Razas por país</h3>
            <nav>
                <a href="~/Home/Index">Listado Alfabético de Razas</a>
            </nav>
        </section>

        <section class="list">
            @foreach (var p in Model)
    {
        <h4>@p.Pais</h4>
        <ul>
            @foreach (var r in p.Perros)
            {
                <li>
                    <a href="~/Raza/@r.Nombre">
                        <img src="imgs_perros/@(r.Id)_0.jpg" />
                        @r.Nombre
                    </a>
                </li>
            }
            
        </ul>
    }
            
        </section>


